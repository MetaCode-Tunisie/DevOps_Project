pipeline{
    agent any
    stages {
        stage("Testing Maven"){
            steps {
                sh "mvn -version"
            }
        }
        stage("Testing JDK"){
            steps {
                sh "java -version"
            }
        }
        stage("Chekout Git"){
            steps {
                echo "Pulling"
            }
        }
        stage('MVN COMPILE') {
            steps{
                 
                sh 'mvn compile'
           
        }
}
stage('Run Tests') {
    steps {
        
            sh 'mvn test' 
      
    }
}
     stage('MVN SONARQUBE') {
            steps{
                 
                sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=sonar'
            }}
        
    }
}
