pipeline {
    agent any
    stages {
        stage("Maven Test") {
            steps {
                sh "mvn -version"
            }
        }
        stage("JDK Test") {
            steps {
                sh "java -version"
            }
        }
        stage("Getting Project From Git") {
            steps {
                echo "Pulling"
                // Ajoutez ici l'étape de récupération du projet Git
            }
        }



        stage('Deploy Nexus') {
            steps {
           // Exécute la commande Maven pour déployer l'artefact sur Nexus
               sh 'mvn deploy -DskipTests=true'
           }
        }
    }
}
